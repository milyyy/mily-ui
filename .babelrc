{
  "presets": [
    ["@babel/preset-env"]
  ],
  "plugins": ["transform-vue-jsx"],
  "env": {
    "utils": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": "commonjs",
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
          }
        ]
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["mily-ui"],
          "alias": {
            "mily-ui/src": "mily-ui/lib"
          }
        }]
      ]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  }
}